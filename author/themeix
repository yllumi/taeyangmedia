        if($(".nf-loadmore").length){
            let str = window.location.href.split('/');
            var categorySlug = str.pop() || str.pop();

                $('.infinite-scroll-3').infiniteScroll({
                path: function path() {
                var pageNumber = this.loadCount + 2;
                return '/category/'+categorySlug+'/pages/' + pageNumber + '/index.html';
                },
                append: '.post-item',
                button: '.nf-loadmore',
                scrollThreshold: false
            });

            $('.infinite-scroll-4').infiniteScroll({
                path: function path() {
                var pageNumber = this.loadCount + 2;
                return '/tag/'+categorySlug+'/pages/' + pageNumber + '/index.html';
                },
                append: '.post-item',
                button: '.nf-loadmore',
                scrollThreshold: false
            });
        }

    </script>

