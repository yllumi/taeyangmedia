  function filter(str) {
      if (!str || str === '') {
          $('.films figure', $showTimes).prop('hidden', false);
      } else {
          $('.films cite', $showTimes).each(function() {
              $(this).parents('figure')
                  .prop('hidden', $(this).text().toLowerCase().indexOf(str.toLowerCase()) &lt; 0);
